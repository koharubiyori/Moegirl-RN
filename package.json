{
  "name": "MoegirlViewer",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "yarn adb:reverse && react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "clear": "cd android && ./gradlew clean",
    "build": "yarn clear && cd android && ./gradlew assembleRelease && cd ../",
    "build:test": "yarn clear && react-native run-android --variant release",
    "adb:reverse": "adb reverse tcp:8081 tcp:8081",
    "adb:push": "adb push android/app/build/outputs/apk/release/app-armeabi-v7a-release.apk sdcard",
    "adb:restart": "adb kill-server && adb start-server",
    "adb:install": "adb install -r android/app/build/outputs/apk/release/app-armeabi-v7a-release.apk",
    "adb:uninstall": "adb uninstall com.moegirlviewer",
    "adb:reinstall": "yarn adb:uninstall && yarn adb:install"
  },
  "dependencies": {
    "@koharubiyori/react-native-orientation": "^3.1.4",
    "@react-native-community/async-storage": "^1.11.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-navigation/drawer": "^5.8.1",
    "@react-navigation/material-top-tabs": "^5.2.13",
    "@react-navigation/native": "^5.4.3",
    "@react-navigation/stack": "^5.4.0",
    "@ubaids/react-native-material-textfield": "^0.16.3",
    "axios": "^0.19.2",
    "color": "^3.1.2",
    "hermes-engine": "0.5.2-rc1",
    "jss": "10.0.3",
    "jss-preset-default": "10.0.3",
    "md5": "^2.2.1",
    "mobx": "^5.15.4",
    "mobx-react-lite": "^2.0.7",
    "moment": "^2.27.0",
    "qs": "^6.9.4",
    "react": "~16.9.0",
    "react-dom": "~16.9.0",
    "react-native": "^0.63.2",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-html-parser": "^0.1.0",
    "react-native-i18n": "^2.0.15",
    "react-native-image-zoom-viewer": "^3.0.1",
    "react-native-navigation-bar-color": "^2.0.1",
    "react-native-paper": "^3.6.0",
    "react-native-reanimated": "^1.9.0",
    "react-native-restart": "^0.0.17",
    "react-native-root-siblings": "^4.0.6",
    "react-native-safe-area-context": "^2.0.3",
    "react-native-screens": "^2.8.0",
    "react-native-snackbar-component": "^1.1.10",
    "react-native-splash-screen": "^3.2.0",
    "react-native-tab-view": "^2.14.4",
    "react-native-vector-icons": "^6.6.0",
    "react-native-webview": "^10.9.0",
    "rn-fetch-blob": "^0.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-decorators": "^7.10.3",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^0.0.7",
    "@types/color": "^3.0.1",
    "@types/jest": "^24.0.24",
    "@types/jquery": "^3.3.38",
    "@types/md5": "^2.2.0",
    "@types/qs": "^6.9.3",
    "@types/react": "~16.9.23",
    "@types/react-native": "^0.61.15",
    "@types/react-native-i18n": "^2.0.0",
    "@types/react-native-material-textfield": "^0.16.3",
    "@types/react-native-snackbar-component": "^1.1.0",
    "@types/react-native-vector-icons": "^6.4.5",
    "@types/react-test-renderer": "16.9.1",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "babel-jest": "^24.9.0",
    "babel-plugin-root-import": "^6.5.0",
    "eslint": "^6.5.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.56.0",
    "prettier": "^1.19.1",
    "react-test-renderer": "16.9.0",
    "typescript": "^3.7.3"
  },
  "jest": {
